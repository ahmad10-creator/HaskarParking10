<script>
const tableBody = document.getElementById('accountsTable');
for (let i = 1; i <= 120; i++) {
    const row = document.createElement('tr');
    const cellName = document.createElement('td');
    const cellPass = document.createElement('td');
    cellName.textContent = `CPUser${i}`;
    cellPass.textContent = `pass${100 + i}`;
    row.appendChild(cellName);
    row.appendChild(cellPass);
    row.addEventListener('click', () => {
        alert(`تم اختيار الحساب: ${cellName.textContent}`);
    });
    tableBody.appendChild(row);
}
</script>
